{
  "name": "angular-cli-app-with-rollup",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc-app-watch": "ngc -w -p src/tsconfig.app.json",
    "ngc-app": "ngc -p src/tsconfig.app.json",
    "rollup-clean-dev": "rimraf out-tsc && rimraf dist-rollup",
    "rollup-clean-prod": "rimraf out-tsc && rimraf dist-rollup-prod",
    "rollup-watch": "rollup -c -w --environment BUILD:dev",
    "rollup-dev": "npm run rollup-clean-dev && npm run ngc-app && npm run rollup-dev-app",
    "rollup-dev-app": "npm-run-all --parallel ngc-app-watch rollup-watch",
    "rollup-prod": "npm run rollup-clean-prod && npm run ngc-app && rollup -c --environment BUILD:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/router": "11.0.5",
    "core-js": "3.3.3",
    "rxjs": "6.5.3",
    "systemjs": "6.1.4",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.0",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.5",
    "browser-sync": "2.26.7",
    "codelyzer": "^6.0.0",
    "compression": "1.7.4",
    "fs-extra": "8.1.0",
    "http2": "3.3.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "3.0.0",
    "rollup": "1.25.2",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-entrypoint-hashmanifest": "0.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-static-site": "0.1.0",
    "rollup-plugin-terser": "5.1.2",
    "strip-json-comments": "3.0.1",
    "ts-node": "8.4.1",
    "tslint": "~6.1.0",
    "typescript": "4.0.5"
  }
}
