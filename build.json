{
    "libs": {
        "polyfills": {
            "entry": "polyfills.js",
            "polyfill": true,
            "needsCommonJS": true
        },
        "@angular/core": {},
        "@angular/common": {},
        "@angular/platform-browser": {},
        "@angular/forms": {},
        "@angular/router": {},
        "rxjs": {
            "sourceMaps": true
        },
        "rxjs/operators": {
            "sourceMaps": true
        },
        "@app/common": {
            "entry": "app/common/index.js",
            "sourceMaps": true
        },
        "dyn": {
            "entry": {
                "dyn1": "app/dyn1/dyn1.module.js",
                "dyn2": "app/dyn2/dyn2.module.js"
            },
            "sourceMaps": true
        },
        "main": {
            "entry": "main.js",
            "sourceMaps": true
        }
    },
    "formats": [
        "system",
        "esm"
    ],
    "importAlias": {
        "app/common": "@app/common"
    },
    "ngcOut": "out-tsc/app",
    "indexHtmlTemplate": "src/index.rollup.html",
    "env": {
        "dev": {
            "dist": "dist-rollup",
            "uglify": false,
            "hash": true,
            "indexHtml": true,
            "serve": true
        },
        "prod": {
            "dist": "dist-rollup-prod",
            "uglify": true,
            "hash": true,
            "indexHtml": true
        }
    }
}